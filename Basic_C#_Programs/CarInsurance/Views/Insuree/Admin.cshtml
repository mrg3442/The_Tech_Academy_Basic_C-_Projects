<div class="card-body">
            @foreach (var coverage in Model)
            {
                    <div class="row mb-3">
                        <div class="col-2">
                            @coverage.FirstName
                        </div>
                        <div class="col">
                            @coverage.LastName
                        </div>
                        <div class="col-4">
                            @coverage.EmailAddress
                        </div>
                        <div class="col">
                            @coverage.Quote
                        </div>
                    </div>

            }